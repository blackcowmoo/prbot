{
  "name": "prbot",
  "version": "0.0.1",
  "repository": "https://github.com/blackcowmoo/prbot.git",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=test nodemon --legacy-watch src/* --exec babel-node --extensions \".ts,.tsx\" src/index.ts",
    "build": "babel src --out-dir build --copy-files --ignore \"**/*.test.ts\" --extensions \".ts,.tsx\"",
    "build:test": "babel src --out-dir build --copy-files --extensions \".ts,.tsx\"",
    "start": "node ./build/index.js",
    "tslint": "tslint --project ./",
    "test": "NODE_ENV=test mocha -r ./mocha.js ./src/test/**/*.test.ts",
    "test:js": "NODE_ENV=test mocha ./build/test/**/*.test.js"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "probot": "^9.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/chai": "^4.2.8",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/mocha": "^7.0.1",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "tslint": "^6.0.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.5"
  }
}
